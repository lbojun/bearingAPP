/* pages/result/result.wxss */
.bg{
  background-color: bisque;
  height: 100%;
  position: absolute;
  width: 220%;
  left: 0px;
  top: 0px;
  z-index: -1;
  opacity: 0.5;
}
.digital{
  width: 50px;
  height: 50px;
}
.titleContainer{
  margin: 40px auto;
}

.return{
  position: absolute;
  top: 20px;
  right: 10px;
  width: 140rpx;
  height: 80rpx;
  line-height: 80rpx;
  border-radius: 10rpx;
  text-align: center;
  background: #337ab7;
  color:#fff;
  margin:auto;
}
.table{
  margin: 0 auto;
  display: flex;
  align-items: center;
}
.th{
  background-color: rgba(135, 170, 235, 0.925);
  text-align: center;
  margin: 2px 0;
  font-size: 15px;
  width: 100px;
  line-height: 20px;
  border:1px blueviolet solid;
  border-radius: 10px;
}
.td{
  text-align: center;
  font-size: 12px;
  width: 200px;
  height: 20px;
  line-height: 20px;
  margin: 2px 1px;
  border: 1px black solid;
  border-radius: 10px;

}
ec-canvas {
  width: 100%;
  height: 100%;
}
.chart-container{
  margin-top: 30px;
  width: 100%;
  height: 600rpx;
}
